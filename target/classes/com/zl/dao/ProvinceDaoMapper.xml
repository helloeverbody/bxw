<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zl.dao.ProvinceDao">
	<select id="queryProvinceById" parameterType="int" resultType="province">
		select id id,Province_name provinceName from Province where id=#{id}
	</select>
</mapper>
